og-description\"\n          >\n            <DialogTitle id=\"alert-dialog-title\">Settings</DialogTitle>\n            <DialogContent>\n              <Grid\n                container\n                direction=\"row\"\n                justify=\"space-evenly\"\n                alignItems=\"center\"\n              >\n                <Grid item xs={8}>\n                  <Typography variant=\"subtitle1\">\n                      Dark Mode\n                  </Typography>\n                </Grid>\n                <Grid item xs={4}>\n                  <Switch\n                    checked={context.state.darkMode}\n                    onChange={\n                    (event) => {\n                      context.toggleDark(!event.target.checked);\n                    }}\n                    value=\"darkMode\"\n                  />\n                </Grid>\n                <Grid item xs={8}>\n                  <Typography variant=\"subtitle1\">\n                      Theme\n                  </Typography>\n                </Grid>\n                <Grid item xs={4}>\n                <div>\n                  <Avatar className={classes.colorPicker} />\n                </div>\n                </Grid>\n              </Grid>\n            </DialogContent>\n            <DialogActions>\n              <Button onClick={close} color=\"primary\">\n                Exit\n              </Button>\n            </DialogActions>\n          </Dialog>\n        </div>\n      )}\n    </SaveConsumer>\n\n  );\n};\n\nsettingsPopup.propTypes = {\n  classes: PropTypes.object.isRequired,\n  isOpen: PropTypes.bool.isRequired,\n  close: PropTypes.func.isRequired,\n\n};\nexport default withStyles(styles)(settingsPopup);\n"},"lineCount":null}},"hash":"0be5d727908a8f17e5884cd3b5a25637","cacheData":{"env":{}}}