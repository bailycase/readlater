n":15}}],"sources":{"components/addPopup.jsx":"import React, { useState } from 'react';\nimport Button from '@material-ui/core/Button';\nimport Dialog from '@material-ui/core/Dialog';\nimport DialogActions from '@material-ui/core/DialogActions';\nimport DialogContent from '@material-ui/core/DialogContent';\nimport DialogTitle from '@material-ui/core/DialogTitle';\nimport TextField from '@material-ui/core/TextField';\nimport { SaveConsumer } from './context';\n\n\nconst addPopup = (props) => {\n  const [title, onTitleChange] = useState(null);\n  const [url, onUrlChange] = useState(null);\n  const handleTitleChange = event => onTitleChange(event.target.value);\n  const handleUrlChange = event => onUrlChange(event.target.value);\n  const data = {\n    title,\n    url,\n  };\n  const {isOpen, close} = props;\n\n  return (\n    <SaveConsumer>\n      {context => (\n        <div>\n          <Dialog\n            open={isOpen}\n            close={close}\n            aria-labelledby=\"alert-dialog-title\"\n            aria-describedby=\"alert-dialog-description\"\n          >\n            <DialogTitle id=\"alert-dialog-title\">Add Item</DialogTitle>\n            <DialogContent>\n              <TextField\n                id=\"standard-name\"\n                label=\"Title\"\n                margin=\"dense\"\n                fullWidth\n                autoFocus\n                onChange={handleTitleChange}\n              />\n              <TextField\n                id=\"standard-name\"\n                label=\"URL\"\n                margin=\"dense\"\n                fullWidth\n                onChange={handleUrlChange}\n              />\n            </DialogContent>\n            <DialogActions>\n              <Button onClick={close} color=\"primary\">\n            Cancel\n              </Button>\n              <Button\n                onClick={() => {\n                  context.addItem(data);\n                  close();\n                }}\n                color=\"primary\"\n                autoFocus\n              >\n            Save\n              </Button>\n            </DialogActions>\n          </Dialog>\n        </div>\n      )}\n    </SaveConsumer>\n  );\n};\n\nexport default addPopup;\n"},"lineCount":null}},"hash":"304520c8a42d6db7964e0a9a5c424a45","cacheData":{"env":{}}}